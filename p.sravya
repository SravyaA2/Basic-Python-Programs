def binary_search(arr,target):
    lb=0
    ub=len(arr)-1
    while lb<=ub:
        mid=(lb+ub)//2
        if arr[mid]==target:
            return mid
        elif arr[mid]<target:
            lb=mid+1
        else:
            ub=mid-1
            return -1
            
arr=[1,2,3,4,5]
target=5
result=binary_search(arr,target)
print("element is found at index:",result)
if result== -1:
    print("element is not found")
