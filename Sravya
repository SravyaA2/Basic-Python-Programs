#get input from user and prtint your 
name = input("enter your name")
print("your name:",name)

#get input from user and print 2 numbers
num1 = input("enter 1st number")
num2 = input("enter 2nd number")
print("1st number:",num1);
print("2nd number:",num2);

#get input from user and add two number
num1 = int(input("enter first number"))
num2 = int(input("enter second number"))
sum = num1 + num2
print("sum of the value",sum);

#concatinate frrst name and last name
a = input("enter first name")
b = input("enter second name")
fullname = a+""+b 
print("full name:",fullname)

#get input from user and swap two numbers without third variable
a = 10;
b = 20;
a = a+b;
b = a-b;
print("a:",a)
print("b:",b)

#To print primitive datatypes

a=10
b=2.13
c=True
print("integer is:",a)
print("Float is:",b)
print("boolean is:",c)

#To print non primitive datatypes

a=10
b=2.13
c=True
dict_1={"one":1,"two":2}
print("integer is:",a)
print("Float is:",b)
print("boolean is:",c)
print("dictionary is:",dict_1)

#To print square of a number

a=int(input("enter a number:"))
square=a*a
print("square of given number:",square)

#To find average of a number

a=float(input("enter first number:"))
b=float(input("enter second number:"))
c=float(input("enter third number:"))
avg=(a+b+c)/3
print("average is:",avg)

#Take user input and multip[ly by 10]

a=int(input("enter a number:"))
b=a*10
print("after multiplying with 10:",b)

#To convert minutes to seconds

a=int(input("enter minutes:"))
sec=a*60;
print("seconds=",sec)

#To multiply floating points

a=float(input("enter first number:"))
b=float(input("enter second number:"))
product=a*b
print("product:",product)
